{
  "bootstrap5": {
    "basic": {
      "theme": "Darkster",
      "primary": "#FF550B",
      "secondary": "#303030",
      "success": "#015668",
      "danger": "#FF304F",
      "info": "#0F81C7",
      "warning": "#0DE2EA",
      "light": "#e8e8e8",
      "dark": "#000000",
      "link-color": "#f0f0f0",
      "headings-font-family": "Hammersmith One:400",
      "body-color": "#cccccc",
      "body-bg": "#000000",
      "enable-shadows": true,
      "font-family-base": "Trebuchet MS:400"
    },
    "advanced": {
      "gray-300": "#000000",
      "gray-800": "#555555",
      "link-hover-color": "darken($link-color,20%)",
      "table-hover-bg": "rgba(255,255,255,.075)",
      "table-border-color": "rgba(255,255,255, 0.3)",
      "input-bg": "$gray-300",
      "input-disabled-bg": "$gray-300",
      "dropdown-bg": "$gray-800",
      "dropdown-divider-bg": "rgba(255,255,255,.15)",
      "dropdown-link-color": "$body-color",
      "dropdown-link-hover-color": "#ffffff",
      "dropdown-link-hover-bg": "$body-bg",
      "nav-tabs-border-color": "rgba(255,255,255, 0.3)",
      "nav-tabs-link-hover-border-color": "$nav-tabs-border-color",
      "nav-tabs-link-active-color": "#ffffff",
      "nav-tabs-link-active-bg": "transparent",
      "nav-tabs-link-active-border-color": "$nav-tabs-border-color",
      "navbar-dark-hover-color": "#ffffff",
      "navbar-light-hover-color": "$gray-800",
      "navbar-light-active-color": "$gray-800",
      "pagination-color": "#ffffff",
      "pagination-bg": "#000000",
      "pagination-border-color": "rgba(255,255,255, 0.6)",
      "pagination-hover-color": "#ffffff",
      "pagination-hover-bg": "$primary",
      "pagination-hover-border-color": "rgba(255,255,255, 0.6)",
      "pagination-active-bg": "lighten($pagination-hover-bg,5%)",
      "pagination-active-border-color": "rgba(255,255,255, 0.6)",
      "pagination-disabled-bg": "lighten($pagination-bg,5%)",
      "pagination-disabled-border-color": "rgba(255,255,255, 0.6)",
      "card-border-color": "rgba(255,255,255, 0.6)",
      "card-bg": "lighten($body-bg, 5%)",
      "popover-bg": "lighten($body-bg,15%)",
      "popover-header-bg": "lighten($body-bg,10%)",
      "popover-border-color": "rgba(255,255,255,.2)",
      "popover-arrow-color": "lighten($body-bg,15%)",
      "tooltip-bg": "lighten($body-bg,15%)",
      "tooltip-arrow-color": "lighten($body-bg,15%)",
      "modal-content-bg": "lighten($body-bg,15%)",
      "modal-header-border-color": "rgba(255,255,255,.2)",
      "modal-content-border-color": "rgba(255,255,255,.2)",
      "progress-bg": "darken($gray-900,5%)",
      "progress-bar-color": "$gray-600",
      "list-group-bg": "lighten($body-bg,5%)",
      "list-group-border-color": "rgba(255,255,255,0.6)",
      "list-group-hover-bg": "lighten($body-bg,10%)",
      "list-group-active-color": "#ffffff",
      "list-group-active-bg": "$list-group-hover-bg",
      "list-group-active-border-color": "$list-group-border-color",
      "list-group-disabled-color": "$gray-800",
      "list-group-disabled-bg": "#000000",
      "list-group-action-color": "#ffffff",
      "breadcrumb-active-color": "$gray-500",
      "breadcrumb-bg": "$gray-700"
    }
  }
}
